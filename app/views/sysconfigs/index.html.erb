<%- model_class = Sysconfig -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:cfgdate) %></th>
      <th><%= model_class.human_attribute_name(:cfgtime) %></th>
      <th><%= model_class.human_attribute_name(:cfgname) %></th>
      <th><%= model_class.human_attribute_name(:cfginteger) %></th>
      <th><%= model_class.human_attribute_name(:cfgstring) %></th>
      <th><%= model_class.human_attribute_name(:cfgtext) %></th>
      <th><%= model_class.human_attribute_name(:lock_version) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @sysconfigs.each do |sysconfig| %>
      <tr>
        <td><%= link_to sysconfig.id, sysconfig_path(sysconfig) %></td>
        <td><%= sysconfig.cfgdate %></td>
        <td><%= sysconfig.cfgtime %></td>
        <td><%= sysconfig.cfgname %></td>
        <td><%= sysconfig.cfginteger %></td>
        <td><%= sysconfig.cfgstring %></td>
        <td><%= sysconfig.cfgtext %></td>
        <td><%= sysconfig.lock_version %></td>
        <td><%=l sysconfig.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_sysconfig_path(sysconfig), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      sysconfig_path(sysconfig),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_sysconfig_path,
            :class => 'btn btn-primary' %>
