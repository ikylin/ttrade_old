<%- model_class = Odsreserf -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:code) %>:</strong></dt>
  <dd><%= @odsreserf.code %></dd>
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @odsreserf.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:plate) %>:</strong></dt>
  <dd><%= @odsreserf.plate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:open) %>:</strong></dt>
  <dd><%= @odsreserf.open %></dd>
  <dt><strong><%= model_class.human_attribute_name(:high) %>:</strong></dt>
  <dd><%= @odsreserf.high %></dd>
  <dt><strong><%= model_class.human_attribute_name(:low) %>:</strong></dt>
  <dd><%= @odsreserf.low %></dd>
  <dt><strong><%= model_class.human_attribute_name(:close) %>:</strong></dt>
  <dd><%= @odsreserf.close %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dprofit) %>:</strong></dt>
  <dd><%= @odsreserf.dprofit %></dd>
  <dt><strong><%= model_class.human_attribute_name(:duration) %>:</strong></dt>
  <dd><%= @odsreserf.duration %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              odsreserves_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_odsreserf_path(@odsreserf), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              odsreserf_path(@odsreserf),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
